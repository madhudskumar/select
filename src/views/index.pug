extends layout/_layout

block content
    .container
        form#select-form(action="_blank" method="GET" )
            div#sb
                label(for='select-box') profile
                select#select-box(name="SB")
                    option(value="settings" selected='selected') Settings
                    option(value="password") password
                    option(value="logout") logout

            button#submit submit
block scripts
    script(src='build.js')
